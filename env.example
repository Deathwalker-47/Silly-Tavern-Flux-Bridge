# Flux LoRA Bridge configuration
# Copy this file to .env and fill only the values you intend to use.
# Never commit your real .env file.

# ---- Core service ----
BRIDGE_PORT=7861
LOG_LEVEL=INFO
LORA_DICT_PATH=master_lora_dict.json
ENABLE_SUMMARIZATION=true
SUMMARY_MAX_LENGTH=300

# ---- Runware (primary image provider) ----
RUNWARE_API_KEY=
RUNWARE_ENDPOINT=https://api.runware.ai/v1
RUNWARE_MODEL=runware:101@1
MAXLORAS_RUNWARE=12

# ---- Hugging Face Space (fallback provider via gradio_client) ----
HF_SPACE_NAME=
HF_TOKEN=
MAXLORAS_HF=10

# ---- Wavespeed ----
WAVESPEED_API_KEY=
MAXLORAS_WAVESPEED=4

# ---- FAL ----
FAL_API_KEY=
MAXLORAS_FAL=3

# ---- Together AI ----
TOGETHER_API_KEY=
MAXLORAS_TOGETHER=2

# ---- Pixel Dojo ----
PIXELDOJO_API_KEY=

# ---- Legacy (optional/unused by default flow) ----
ATLASCLOUD_API_KEY=

# ---- OpenAI-compatible You.com proxy endpoints ----
YOU_COM_API_KEY=
YOU_COM_DEFAULT_AGENT=
